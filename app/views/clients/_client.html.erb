
<div id="<%= dom_id client %>" >
  <tr >
    <td><%= client.id %></td>
    <td><%= client.name %></td>
    <td><%= client.age %></td>
    <td><%= client.movies.last %></td>
    
  
    <td><%= link_to edit_client_path(client) do %>
        <i class="bi bi-pencil text-light" ></i>
        <%end %>
    </td>
    
    <td><%= button_to client_path(client), method: :delete  do %>
          <i class="bi bi-trash"></i>
        <%end %>
    </td>
    
  </tr>
</div>
